{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowAccountBAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::XXXXXXXXXXX:root"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": ""arn:aws:s3::: s3-cli-access-limit /*"
        },
        {
            "Sid": "AllowAccountBListBucket",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::XXXXXXXXXX:root"
            },
            "Action": "s3:ListBucket",
            "Resource": ""arn:aws:s3::: s3-cli-access-limit/*"
        }
    ]
}
